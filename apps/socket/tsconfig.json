{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["es2022", "dom"],
        "outDir": "build",
        "rootDir": "darryl",
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "exactOptionalPropertyTypes": true,
        "noPropertyAccessFromIndexSignature": true
    },
    "include": [
        "./darryl/**/*"
    ]
}
